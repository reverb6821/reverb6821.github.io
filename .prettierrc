{
    "jsxSingleQuote": true,
    "singleQuote": true,
    "semi": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "printWidth": 100,
    "bracketSameLine": false,
    "useTabs": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "importOrder": [
        "^react",
        "^\\w",
        "/types/",
        "/hooks/",
        "/store/",
        "/views/",
        "/components/",
        "/assets/",
        "^./",
        "css$"
      ],
      "importOrderSeparation": true,
      "importOrderSortSpecifiers": true
  }